{
  "extends": "airbnb-base",
  "env": {
    "browser": true
  },
  "globals": {
    "WebSocket": true,
    "document": true,
    "google": true,
    "Paho": true,
    "YT": true,
    "Mosquitto": true
  },
  "plugins": [
    "lit"
  ],
  "rules": {
    "class-methods-use-this": 0,
    "no-underscore-dangle": 0
  }
}
